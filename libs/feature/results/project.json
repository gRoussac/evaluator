{
  "projectType": "library",
  "sourceRoot": "libs/feature/results/src",
  "prefix": "evaluator",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/feature/results"],
      "options": {
        "jestConfig": "libs/feature/results/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/feature/results/**/*.ts",
          "libs/feature/results/**/*.html"
        ]
      }
    }
  },
  "tags": ["results"]
}
