    "@nestjs/platform-express": "^9.0.0",
    "@nguniversal/express-engine": "^14.2.0",
    "@nrwl/angular": "14.7.5",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.0.2",
    "express": "^4.15.2",
    "highlight.js": "^11.6.0",
    "http-proxy-middleware": "2.0.6",
    "jquery": "3.6.1",
    "jsdom": "20.0.0",
    "promise-worker": "^2.0.1",
    "puppeteer": "^17.1.3",
    "reflect-metadata": "^0.1.13",
